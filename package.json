{
    "name": "ionx",
    "version": "1.0.0",
    "description": "Components and extensions for Ionic",
    "sourceDependencyDir": "src/components",
    "files": [
        "src/"
    ],
    "scripts": {
        "build": "stencil build --docs",
        "start": "stencil build --dev --watch --serve",
        "serve": "stencil serve --root www",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate"
    },
    "peerDependencies": {
        "@co.mmons/js-intl": "github:co-mmons/js-intl",
        "@co.mmons/js-utils": "github:co-mmons/js-utils",
        "@ionic/core": "^5.3.1",
        "@stencil/core": "^1.17.3",
        "@stencil/sass": "^1.3.2",
        "tippy.js": "^6.2.6"
    },
    "devDependencies": {
        "@capacitor/core": "^2.4.1",
        "@co.mmons/js-intl": "github:co-mmons/js-intl",
        "@co.mmons/js-utils": "github:co-mmons/js-utils",
        "@ionic/core": "^5.3.1",
        "@stencil/core": "^2.0.1",
        "@stencil/forms": "github:adamdbradley/stencil-forms",
        "@stencil/sass": "^1.3.2",
        "@types/jest": "^26.0.10",
        "bignumber.js": "^9.0.0",
        "rxjs": "^6.6.2",
        "tippy.js": "^6.2.6",
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "tippy.js": "*",
        "fast-equals": "^2.0.0",
        "ts-error": "^1.0.6"
    },
    "intlBundleItems": [
        {
            "type": "message",
            "path": "./src/components/forms/intl/{{LOCALE}}.json",
            "module": "ionx/forms"
        }
    ]
}
